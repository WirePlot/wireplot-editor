{
  "name": "projectmodeler",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.js",
  "build": {
    "appId": "com.fasung.wireplot",
    "productName": "WirePlot",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "win": {
      "target": "nsis"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "dist": "npm run build && electron-builder",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron": "electron electron/main.js",
    "dev:electron": "concurrently \"npm run dev\" \"cross-env ELECTRON_DEV=true npm run electron\"",
    "start:electron": "npm run build && cross-env ELECTRON_DEV=false npm run electron"
  },
  "dependencies": {
    "@jsonforms/core": "^3.6.0",
    "@jsonforms/material-renderers": "^3.6.0",
    "@jsonforms/react": "^3.6.0",
    "@reduxjs/toolkit": "^2.9.2",
    "@xyflow/react": "^12.9.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.5",
    "reselect": "^5.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "vite": "^6.4.1"
  }
}